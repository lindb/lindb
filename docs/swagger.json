        "/log/list": {
            "get": {
                "description": "return all log files in log dir.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "State"
                ],
                "summary": "list log files",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object"
                        }
                    },
                    "404": {
                        "description": "not found",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "internal error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/log/view": {
            "get": {
                "description": "return last N lines in log file.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "text/plain"
                ],
                "tags": [
                    "State"
                ],
                "summary": "tail log file",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
